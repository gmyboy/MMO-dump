<?xml version="1.0" encoding="UTF-8"?>
<themes>
    <theme name="advanced-search-dialog" ref="dialoglayout" overwrite="true">
      <param name="border"><border>11,0,0,0</border></param>
      <!-- Value -->
     <theme name="label-value-small" ref="label">
        <param name="background"><image>mobile.box.dark3.50</image></param>
        <param name="textAlignment"><enum type="alignment">left</enum></param>
        <param name="border"><border>6</border></param>
        <param name="minWidth"><int>120</int></param>
      </theme>
      <theme name="label-value-medium" ref="label">
        <param name="background"><image>mobile.box.dark3.50</image></param>
        <param name="textAlignment"><enum type="alignment">left</enum></param>
        <param name="border"><border>6</border></param>
        <param name="minWidth"><int>160</int></param>
      </theme>
      <theme name="label-value-medium2" ref="label">
        <param name="background"><image>mobile.box.dark3.50</image></param>
        <param name="textAlignment"><enum type="alignment">left</enum></param>
        <param name="minWidth"><int>220</int></param>
      </theme>
      <!-- Titles -->
      <theme name="label-title-medium" ref="label-value-medium">
        <param name="background"><image>ui-header.background</image></param>
      </theme>
      <theme name="label-title-medium2" ref="label-value-medium2">
        <param name="background"><image>mobile.box.dark3.50</image></param>
        <param name="font"><font>med-noshadow-18-white</font></param>
        <param name="textAlignment"><enum type="alignment">center</enum></param>
      </theme>
      <!-- Buttons -->
      <theme name="label-button-small" ref="label-value-small">
        <param name="background"><image>ui-button.background</image></param>
      </theme>
      <theme name="label-button-small2" ref="button">
        <param name="background"><image>ui-button.background</image></param>
        <param name="minWidth"><int>96</int></param>
      </theme>
      <theme name="label-button-small2-centered" ref="label-button-small2">
        <param name="textAlignment"><enum type="alignment">center</enum></param>
      </theme>
      <theme name="combobox" ref="combobox">
        <param name="minWidth"><int>300</int></param>
        <param name="minHeight"><int>50</int></param>
      </theme>
      <theme name="valueadjuster" ref="mobile-valueadjuster" allowWildcard="true">
        <param name="minHeight"><int>50</int></param>
      </theme>
      <theme name="editfield" ref="editfield">
        <param name="textAlignment"><enum type="alignment">CENTER</enum></param>
        <param name="maxWidth"><int>300</int></param>
      </theme>
      <theme name="togglebutton" ref="togglebutton">
        <param name="minHeight"><int>50</int></param>
      </theme>
    </theme>
    
    <theme name="advanced-search-dialog-alt" ref="advanced-search-dialog">
      <param name="border"><border>0,10,0,10</border></param>
      <theme name="valueadjuster" ref="mobile-valueadjuster" overwrite="true"/>
      <theme name="combobox" ref="mobile-combobox" overwrite="true">
      <param name="background"><image>mobile.box.dark1.50</image></param>
        <param name="minHeight"><int>60</int></param>
      </theme>
      <theme name="editfield" ref="editfield">
        <param name="maxWidth"><int>300</int></param>
      </theme>
      <theme name="button" ref="-defaults">
        <param name="minWidth"><int>100</int></param>
        <param name="width"><int>100</int></param>
        <param name="border"><border>5,0</border></param>
        <param name="textAlignment"><enum type="alignment">LEFT</enum></param>
      </theme>
      <theme name="label" ref="label">
        <param name="font"><font>med-noshadow-18-white</font></param>
      </theme>
      <theme name="label-value-small2" ref="label">
        <param name="background"><image>mobile.box.dark1.50</image></param>
        <param name="textAlignment"><enum type="alignment">LEFT</enum></param>
        <param name="border"><border>5</border></param>
        <param name="minWidth"><int>150</int></param>
        <param name="minHeight"><int>46</int></param>
        <param name="font"><font>med-noshadow-18-white</font></param>
      </theme>
      <theme name="label-button-small2" ref="label-value-small2">
        <param name="background"><image>mobile.box.light1.50</image></param>
        <param name="minWidth"><int>150</int></param>
        <param name="minHeight"><int>46</int></param>
      </theme>
      <theme name="label-button-small2-centered" ref="label">
        <param name="textAlignment"><enum type="alignment">CENTER</enum></param>
        <param name="background"><image>mobile.box.dark1.50</image></param>
        <param name="minHeight"><int>60</int></param>
        <param name="minWidth"><int>150</int></param>
        <param name="font"><font>med-noshadow-18-white</font></param>
      </theme>
      <theme name="label-title-medium2" ref="label">
        <param name="background"><image>mobile.box.dark3.50</image></param>
        <param name="font"><font>med-noshadow-18-white</font></param>
        <param name="textAlignment"><enum type="alignment">LEFT</enum></param>
        <param name="minWidth"><int>245</int></param>
      </theme>
    </theme>

	
    <theme name="advanced-search-frame" ref="mobile-panel" overwrite="true">
      <theme name="advanced-search-dialog" ref="advanced-search-dialog-alt"/>
    </theme>
</themes>